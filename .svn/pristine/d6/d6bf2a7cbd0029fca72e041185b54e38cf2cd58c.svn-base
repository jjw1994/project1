header{
    width: 100%;
    min-width: 1000px;
    height: 80px;
    background-color: #fff;
}
.head{
	position: relative;
	height: 50px;
	padding: 15px 0 15px 22px;
}
.logo{
	float:left; 
    width: 50px;
    height:50px;
    background-image: url("../../Home/images/userHead.jpg");
    background-size: contain;
    cursor: pointer;
}
.search{
	position: relative;
	left: 60px;
}
.search input{
	outline: none;
	width: 400px;
	height: 48px;
	border-radius: 24px;
	padding-left: 20px;
	border:1px solid rgba(0, 0, 0, 0.23);
}
.search-icon{
	position: relative;
    top: 5px;
    left: -45px;
    font-size: 23px;
    color: rgba(0, 0, 0, 0.5);
}
nav{
	width: 100%;
    min-width: 1000px;
    height: 40px;
    background-color: #333;
}
.bar_left{
	width: 150px;
	margin-left: 20px;
	float: left;
	line-height: 40px;
	color: #fff;
}
.bar_right{
	float: right;
}
.setlist li{
	float: left;
	height: 40px;
	width: 80px;
}

.li1{background-color: rgba(0, 161, 133, 0.22);}
.li2{background-color: rgba(0, 161, 133, 0.42);}
.li3{background-color: rgba(0, 161, 133, 0.57);}
.li4{background-color: rgba(0, 161, 133, 0.76);}
.li5{background-color: rgb(0, 161, 133);}

.seticon{
	color: #fff;
	line-height: 40px;
	font-size: 20px;
	margin-left: 30px;
}
.eite{
	font-size: 24px;
    font-weight: 600;
    position: relative;
    top: -3px;
}